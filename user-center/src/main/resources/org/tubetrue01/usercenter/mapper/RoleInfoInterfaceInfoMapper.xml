<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.tubetrue01.usercenter.mapper.RoleInfoInterfaceInfoMapper">
    <select id="selectListByRoleId" parameterType="int" resultType="int">
        SELECT interface_id
        FROM role_interface_info
        WHERE role_id = #{roleId}
    </select>
</mapper>
